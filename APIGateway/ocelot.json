{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/AllergyMasters",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 9001
        }
      ],
      // Upstream : source of call
      "UpstreamPathTemplate": "/GetAllergyMasters",
      "UpstreamHttpMethod": [ "GET" ] // We can include post,put, delete seperated by comma
    },

        {
          // Downstream: what we are willing to call
          "DownstreamPathTemplate": "/api/AllergyMasters/{id}",
          "DownstreamScheme": "http",
          "DownstreamHostAndPorts": [
            {
              "Host": "localhost",
              "Port": 9001
            }
          ],
          // Upstream : source of call
          "UpstreamPathTemplate": "/GetAllergyMastersbyId/{id}",
          "UpstreamHttpMethod": [ "GET" ] // We can include post,put, delete seperated by comma
        }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:9003"
  }
}

